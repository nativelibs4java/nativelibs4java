<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+"
  codebase="http://jnaerator.sourceforge.net/webstart/jna/3.2.7"
  href="jnidispatch.jnlp"
>
	<information>
	  <title>JNA's jnidispatch library</title>
	  <vendor>JNA</vendor>
	  <homepage href="http://jna.dev.java.net/" />
	  <description>JNA's jnidispatch library</description>
	</information>
	<security>
	  <all-permissions/>
	</security>
	
	<resources os="Windows" arch="x86">
		<property name="jna.library.path" value="C:\Windows\System32"/>
    <nativelib href="jna-win32-x86.jar"/>
	</resources>
	<resources os="Windows" arch="amd64">
		<property name="jna.library.path" value="C:\Windows\SysWOW64"/>
    <nativelib href="jna-win32-amd64.jar"/>
	</resources>
	
	<resources os="FreeBSD" arch="i386">
		<nativelib href="jna-freebsd-i386.jar"/>
	</resources>
	<resources os="FreeBSD" arch="amd64">
		<nativelib href="jna-freebsd-amd64.jar"/>
	</resources>
	
	<resources os="Linux" arch="i386">
		<nativelib href="jna-linux-i386.jar"/>
	</resources>
	<resources os="Linux" arch="amd64">
		<nativelib href="jna-linux-amd64.jar"/>
	</resources>
	<resources os="Linux" arch="ia64">
		<nativelib href="jna-linux-ia64.jar"/>
	</resources>
	
	<resources os="SunOS" arch="x86">
		<nativelib href="jna-sunos-x86.jar"/>
	</resources>
	<resources os="SunOS" arch="amd64">
		<nativelib href="jna-sunos-amd64.jar"/>
	</resources>
	<resources os="SunOS" arch="sparc">
		<nativelib href="jna-sunos-sparc.jar"/>
	</resources>
	<resources os="SunOS" arch="sparcv9">
		<nativelib href="jna-sunos-sparcv9.jar"/>
	</resources>
	
	<resources os="Mac">
		<nativelib href="jna-darwin.jar"/>
	</resources>
	
	<component-desc/>
</jnlp>
